{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "airbnb",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "plugin:prettier/recommended"
    ],
    "plugins": ["react", "react-hooks", "prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "react/react-in-jsx-scope": "off"
    }
}
